{
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "files.insertFinalNewline": true
    },
    "basedpyright.analysis.inlayHints.genericTypes": true,
    "basedpyright.importStrategy": "useBundled",
    "cSpell.enabledFileTypes": {
        "plaintext": false
    },
    "cSpell.userWords": [
        "chezmoi",
        "Macbook",
        "mktemp",
        "moreutils",
        "pipefail",
        "prereqs",
        "pyproject",
        "pyright",
        "pyrightconfig",
        "ruff",
        "ruff.toml",
        "ruffconfig",
        "ryancwallace",
        "Ryans",
        "vscode",
        "vscodium",
        "zlogin",
        "zprofile",
        "zsh",
        "zshrc"
    ],
    "diffEditor.renderSideBySide": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "always",
        "source.organizeImports": "always"
    },
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.fontFamily": "'FiraCode-Retina'",
    "editor.fontLigatures": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.hover.enabled": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.size": "fill",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        120
    ],
    "editor.stickyScroll.enabled": false,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.enableDragAndDrop": false,
    "explorer.excludeGitIgnore": true,
    "explorer.sortOrder": "type",
    "files.associations": {
        "*.py": "python",
        "*.tsx": "typescriptreact"
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.insertFinalNewline": false,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4o",
    "gitlens.views.commitDetails.files.layout": "tree",
    "jupyter.askForKernelRestart": false,
    "jupyter.logging.level": "warn",
    "notebook.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "notebook.defaultFormatter": "charliermarsh.ruff",
    "notebook.formatOnSave.enabled": true,
    "problems.sortOrder": "position",
    "python.experiments.enabled": false,
    "python.testing.cwd": "${workspaceFolder}",
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.pytestEnabled": true,
    "remote.autoForwardPortsSource": "hybrid",
    "ruff.lineLength": 120,
    "ruff.lint.preview": true,
    "search.sortOrder": "type",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.scrollback": 1000000,
    "terminal.integrated.tabs.hideCondition": "never",
    "window.commandCenter": false,
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#4040407c"
    },
    "workbench.colorTheme": "One Dark Pro",
    "workbench.editor.tabSizing": "shrink",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.navigationControl.enabled": false,
    "workbench.productIconTheme": "material-product-icons",
    "workbench.secondarySideBar.defaultVisibility": "visible",
    "workbench.secondarySideBar.showLabels": false,
    "workbench.startupEditor": "none",
    "zenMode.hideLineNumbers": false,
    "zenMode.showTabs": "single"
}